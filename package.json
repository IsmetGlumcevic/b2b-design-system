{
  "name": "b2b-design-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/build-theme.mjs && next dev",
    "build": "node scripts/build-theme.mjs && next build",
    "start": "next start",
    "lint": "eslint",
    "icons:line": "svgr --out-dir src/components/icons/Line --config-file svgr.config.js -- src/lib/icons/Line",
    "icons:solid": "svgr --out-dir src/components/icons/Solid --config-file svgr.config.js -- src/lib/icons/Solid",
    "icons:duotone": "svgr --out-dir src/components/icons/Duotone --config-file svgr.config.js -- src/lib/icons/Duotone",
    "icons:duocolor": "svgr --out-dir src/components/icons/Duocolor --config-file svgr.config.js -- src/lib/icons/Duocolor",
    "icons": "npm run icons:line && npm run icons:solid && npm run icons:duotone && npm run icons:duocolor",
    "build:theme": "node scripts/build-theme.mjs"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
